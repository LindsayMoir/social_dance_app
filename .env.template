# Environment Variable Template
# Copy this to src/.env and fill in real values

# Production Database (Render)
RENDER_PG_HOST=<your_render_prod_host>
RENDER_PG_DB=<your_render_prod_db>
RENDER_PG_USER=<your_render_prod_user>
RENDER_PG_PASS=<your_render_prod_password>
RENDER_EXTERNAL_DB_URL=<your_render_prod_external_url>
RENDER_INTERNAL_DB_URL=<your_render_prod_internal_url>

# Dev Database (Render)
RENDER_DEV_PG_HOST=<your_render_dev_host>
RENDER_DEV_PG_DB=<your_render_dev_db>
RENDER_DEV_PG_USER=<your_render_dev_user>
RENDER_DEV_PG_PASS=<your_render_dev_password>
RENDER_DEV_EXTERNAL_DB_URL=<your_render_dev_external_url>
RENDER_DEV_INTERNAL_DB_URL=<your_render_dev_internal_url>

# Local Database
DATABASE_HOST=localhost
DATABASE_NAME=social_dance_db
DATABASE_USER=postgres
DATABASE_PASSWORD=<your_local_password>
DATABASE_CONNECTION_STRING=postgresql://postgres:<password>@localhost/social_dance_db

# API Keys
ANTHROPIC_API_KEY=<your_key>
# ... etc
